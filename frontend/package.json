{
  "name": "bulk-image-cleaner-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"webflow extension serve\" \"npm run watch-webpack\" \"npm run watch-css\"",
    "build": "npm run build-webpack && npm run build-css && webflow extension bundle",
    "watch-webpack": "webpack --config webpack.config.mjs --mode development --watch",
    "build-webpack": "webpack --config webpack.config.mjs --mode production",
    "watch-css": "tailwindcss -i ./src/input.css -o ./public/output.css --watch",
    "build-css": "tailwindcss -i ./src/input.css -o ./public/output.css"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "tailwindcss": "^4.1.18",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@webflow/designer-extension-typings": "^2.0.2",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  }
}
